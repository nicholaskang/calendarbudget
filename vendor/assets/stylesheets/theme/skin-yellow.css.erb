/****************************/
/* Theme name  : Brave      */
/* Author name : Ashok      */
/****************************/
/******************/
/* Theme Skin     */
/******************/
::selection {
  background-color: #fcd419;
}
::-moz-selection {
  background-color: #fcd419;
}
/***************/
/* Link Colors */
/***************/
a {
  color: #dfb803;
}
a:hover {
  color: #fcd419;
}
/*****************************/
/* Default Theme Color Class */
/*****************************/
.color {
  color: #33CC33 !important;
}
a.color {
  color: #33CC33 !important;
}
a.color:hover,
a.color:focus {
  color: #dfb803;
}
.bg-color {
  background-color: #33CC33 !important;
}
.br-color {
  border: 1px solid #33CC33;
}
.dropcap-1 {
  color: #33CC33;
}
.dropcap-2,
.dropcap-3 {
  background: #33CC33;
}
blockquote {
  border-color: #33CC33;
}
blockquote.blockquote-reverse {
  border-color: #33CC33;
}
/***************/
/* List styles */
/***************/
.list-1 li:before,
.list-2 li:before,
.list-3 li:before,
.list-4 li:before,
.list-5 li:before,
.list-6 li:before,
.list-7 li:before,
.list-8 li:before,
.list-9 li:before {
  color: #33CC33;
}
/*********************************/
/* Theme Button, Label and Badge */
/*********************************/
.btn.btn-color {
  color: #ffffff;
  background: #33CC33;
  border: 1px solid #dfb803;
}
.btn.btn-color:hover,
.btn.btn-color:focus,
.btn.btn-color.active,
.btn.btn-color:active {
  background: #dfb803;
  color: #ffffff;
}
.btn.btn-color.btn-outline {
  background: transparent !important;
  border: 1px solid #dfb803 !important;
  color: #dfb803;
}
.label.label-color {
  background: #33CC33;
  color: #ffffff;
}
.badge.badge-color {
  background: #33CC33;
  color: #ffffff;
}
.highlight-color {
  background: #33CC33;
}
/*****************/
/* Dropdown menu */
/*****************/
.dropdown-menu > li.active > a {
  background-color: #33CC33;
}
.dropdown-menu > li.active > a:hover {
  background-color: #33CC33;
}
.dropdown-menu > li.active > a:focus {
  background-color: #33CC33;
}
/**************/
/* Pagination */
/**************/
.pagination li a {
  color: #555555;
}
.pagination li a:hover {
  background-color: #33CC33;
  border-color: #dfb803;
  color: #ffffff;
}
.pagination li.active a {
  background-color: #33CC33;
  border-color: #dfb803;
}
.pagination li.active a:hover {
  background-color: #33CC33;
  border-color: #dfb803;
}
.pagination li.active a:focus {
  background-color: #33CC33;
  border-color: #dfb803;
}
.pagination li.active span {
  background-color: #33CC33;
  border-color: #dfb803;
}
.pagination li.active span:hover {
  background-color: #33CC33;
  border-color: #dfb803;
}
.pagination li.active span:focus {
  background-color: #33CC33;
  border-color: #dfb803;
}
/*********/
/* Pager */
/*********/
.pager li a {
  background-color: #33CC33;
  color: #ffffff;
  border-color: #dfb803;
}
.pager li a:hover {
  background: #dfb803;
}
/********************/
/* Nav Tabs Default */
/********************/
.nav-tabs > li > a {
  color: #555555;
}
.nav-tabs > li > a:hover {
  background: #33CC33;
  color: #ffffff;
  border-color: #33CC33;
}
.nav-tabs > li.active > a {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs > li.active > a:hover {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs > li.open > a {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs > li.open > a:hover {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs > li.open > a:focus {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
/*********************/
/* Nav tabs style #1 */
/*********************/
.nav-tabs-one .nav-tabs > li > a {
  color: #555555;
}
.nav-tabs-one .nav-tabs > li > a:hover {
  background: #33CC33;
  color: #ffffff;
  border-color: #33CC33;
}
.nav-tabs-one .nav-tabs > li.active > a {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs-one .nav-tabs > li.active > a:hover {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs-one .nav-tabs > li.open > a {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs-one .nav-tabs > li.open > a:hover {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-tabs-one .nav-tabs > li.open > a:focus {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
/*********************/
/* Nav tabs style #2 */
/*********************/
.nav-tabs-two .nav-tabs > li > a {
  color: #555555;
}
.nav-tabs-two .nav-tabs > li > a:hover {
  border-top: 2px solid #33CC33;
}
.nav-tabs-two .nav-tabs > li.active > a {
  border-top: 2px solid #33CC33;
}
.nav-tabs-two .nav-tabs > li.active > a:hover {
  border-top: 2px solid #33CC33;
}
.nav-tabs-two .nav-tabs > li.open > a {
  border-top: 2px solid #33CC33;
}
.nav-tabs-two .nav-tabs > li.open > a:hover {
  border-top: 2px solid #33CC33;
}
.nav-tabs-two .nav-tabs > li.open > a:focus {
  border-top: 2px solid #33CC33;
}
/*********************/
/* Nav tabs style #3 */
/*********************/
.nav-tabs-three .nav li a:hover {
  background: #33CC33;
  border-color: #33CC33;
}
.nav-tabs-three .nav li.active a {
  border-color: #33CC33;
  background: #33CC33;
}
/*********/
/* Panel */
/*********/
.panel.panel-default {
  border-color: #dfb803;
}
.panel.panel-default .panel-heading {
  background: #33CC33;
  border-color: #dfb803;
  color: #ffffff;
}
.panel.panel-default .panel-heading h4 a {
  color: #ffffff;
}
.panel.panel-default .panel-heading h4 a:hover {
  color: #ffffff;
}
.panel.panel-default .panel-footer {
  background: #33CC33;
  border-color: #dfb803;
  color: #ffffff;
}
/*************/
/* Nav Pills */
/*************/
.nav-pills > li > a {
  background: #ffffff;
  border: 1px solid #dddddd;
  color: #555555;
}
.nav-pills > li > a:hover {
  background: #33CC33;
  color: #ffffff;
  border-color: #33CC33;
}
.nav-pills > li.active > a {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-pills > li.active > a:hover {
  background-color: #dfb803;
  color: #ffffff;
}
.nav-pills > li.open > a {
  background-color: #33CC33;
  border-color: #33CC33;
  color: #ffffff;
}
.nav-pills > li.open > a:hover {
  background-color: #dfb803;
  color: #ffffff;
}
.nav-pills > li.open > a:focus {
  background-color: #33CC33;
  border-color: #dfb803;
  color: #ffffff;
}
/**************/
/* List group */
/**************/
.list-group a:hover {
  background: #f7f7f7;
  color: #555555;
}
.list-group a:hover .list-group-item-heading {
  color: #555555;
}
.list-group a.active {
  background: #33CC33;
  border-color: #dfb803;
}
.list-group a.active:hover {
  background: #33CC33;
  border-color: #dfb803;
  color: #ffffff;
}
.list-group a.active:hover .list-group-item-heading {
  color: #ffffff;
}
.list-group a.active:hover .list-group-item-text {
  color: #ffffff;
}
.list-group a.active .list-group-item-heading {
  color: #ffffff;
}
.list-group a.active .list-group-item-text {
  color: #ffffff;
}
/**********************/
/* Brand - theme color */
/**********************/
.brand-color a i {
  color: #33CC33;
}
.brand-color a i:hover {
  color: #c6a303;
}
/****************************/
/* Brand - theme background */
/****************************/
.brand-bg-color a i {
  color: #ffffff;
  background: #33CC33;
}
.brand-bg-color a i:hover {
  color: #ffffff !important;
  background: #c6a303;
}
/* Owl carousel */
.owl-theme .owl-controls .owl-page.active span,
.owl-theme .owl-controls.clickable .owl-page:hover span {
  background: #33CC33;
}
.owl-nav a i:hover {
  background: #33CC33;
  border-color: #dfb803;
}
/*************************/
/* Header one            */
/* Logo & Navigation bar */
/*************************/
.header-1 .logo h1 a i {
  color: #33CC33;
}
.header-1 .navi {
  background: #33CC33;
  border-bottom: 3px solid #f8cd03;
}
.header-1 .navi .navy ul > li.has-sub > a:after {
  color: #dfb803;
}
.header-1 .navi .navy > ul > li > ul {
  border-top: 2px solid #33CC33;
  border-bottom: 2px solid #33CC33;
}
.header-1 .navi .navy ul ul ul {
  border-top: 2px solid #33CC33;
  border-bottom: 2px solid #33CC33;
}
.header-1 .navi .navy #menu-button {
  background: #33CC33;
}
.header-1 .navi .navy ul .submenu-button:after {
  color: #33CC33;
}
/*************************/
/* Header two            */
/* Logo & Navigation bar */
/*************************/
.header-2 .logo h1 a i {
  color: #33CC33;
}
.header-2 .navy ul > li.has-sub > a:after {
  color: #33CC33;
}
.header-2 .navy > ul > li > ul {
  border-top: 2px solid #33CC33;
  border-bottom: 2px solid #33CC33;
}
.header-2 .navy ul ul ul {
  border-top: 2px solid #33CC33;
  border-bottom: 2px solid #33CC33;
}
.header-2 .navy #menu-button {
  background: #33CC33;
}
.header-2 .navy ul .submenu-button:after {
  color: #33CC33;
}
/* Block heading */
.block-heading-two h3 span {
  border-color: #33CC33;
}
.block-heading-three h3 {
  border-color: #33CC33;
}
/* Team Four */
.team-four .team-member:hover .team-details {
  background: #33CC33;
  border: 1px solid #dfb803;
}
/* Image style #1 */
.image-style-one .image-caption a {
  background: #33CC33;
}
/* Image style #2 */
.image-style-two .image-caption a {
  background: #33CC33;
}
/* Image style #5 */
.image-style-five .image-caption h4 {
  border-color: #33CC33;
}
/* Image style #6 */
.image-style-six .image-caption {
  background: #33CC33;
}
.image-style-six .image-caption span a {
  color: #d5b003;
}
/* Image style #7 */
.image-style-seven .image-caption a i {
  background: #33CC33;
}
.image-style-seven .image-caption a i:hover {
  background: #dfb803;
}
/* Facts */
.facts .fact-value span {
  color: #33CC33;
}
.facts .fact-content p span {
  color: #dfb803;
}
/* Pricing table #1 */
.pricing-one .pricing-item {
  border-top: 3px solid #33CC33;
}
/* Pricing table #2 */
.pricing-two i {
  color: #33CC33;
}
/* Pricing table #3 */
.pricing-three h4 span {
  color: #33CC33;
}
/* Image Box #8 */
.img-box-8 .img-box-8-item:hover .img-box-8-cap {
  background: #33CC33;
}
/* V Card */
.v-card header ul li.active a {
  background: #33CC33;
  border-color: #dfb803;
}
.v-card header ul li.active a:hover {
  background: #33CC33;
}
.v-card header ul li a:hover {
  background: #33CC33;
  border-color: #dfb803;
  color: #ffffff;
}
/* Letter Head */
.letter-head {
  border-top: 3px solid #33CC33;
  border-bottom: 3px solid #33CC33;
}
/* Box shadow block */
.box-shadow-block {
  border-top: 2px solid #33CC33;
}
/* Block quote #2 */
.quote-two .quote-two-content span {
  background: #33CC33;
}
/* Client block #1 */
.client-one .c1-item .img-hover {
  background: #33CC33;
}
/* Accordion */
#accordion-alt3 .panel .panel-heading h4 a i {
  background: #33CC33;
}
/* Blog one */
.blog-one .blog-one-content .blog-bor {
  background: #33CC33;
}
/* Blog masonry */
.blog-masonry .item .grid-entry .grid-img i.hover-icon {
  background: #33CC33;
}
.blog-masonry .item .grid-entry .bor {
  background: #33CC33;
}
/* Parallax Slider */
.da-dots span {
  background: #33CC33;
}
.da-arrows span {
  background: #33CC33;
}
/* Bootstrap carousel*/
.carousel .carousel-indicators .active {
  background-color: #33CC33;
}
/* Resume */
.resume .main-block {
  background: #33CC33 url("<%= asset_path('theme/overlay.png') %>") repeat;
  background-size: 252px 230px;
}
/* Smart forms */
/* Rating */
.smart-forms .rating:hover .rating-star:hover,
.smart-forms .rating:hover .rating-star:hover ~ .rating-star,
.smart-forms .rating-input:checked ~ .rating-star {
  color: #33CC33;
}
/* Toggle switch */
.smart-forms .switch > input:checked + label {
  background: #33CC33;
  border-color: #33CC33;
}
.smart-forms .switch > input:checked + label:after {
  color: #33CC33;
}
.smart-forms .switch > input:checked:focus + label {
  background: #33CC33;
  border-color: #33CC33;
}
/* Date & Time picker */
.ui-datepicker-today a,
.ui-datepicker-today a:hover,
.ui-datepicker .ui-state-highlight {
  background: #33CC33 !important;
}
.smart-forms .ui-slider .ui-slider-handle {
  border-color: #33CC33;
}
.smart-forms .ui-slider .ui-slider-handle:before {
  background: #33CC33;
}
/* Index slim */
.slim-feature .feature-content ul li:before {
  color: #33CC33;
}
/* Ecommerce ui #4 */
.ecom-ui-four:hover {
  border: 2px solid #33CC33;
}
/* Ecommerce ui #5 */
.ecom-ui-five {
  border: 2px solid #33CC33;
}
.ecom-ui-five .p-media a {
  border-right: 2px solid #33CC33;
}
/* Ecommerce ui #6 */
.ecom-ui-six:hover {
  border-color: #33CC33;
}
/* Wedding */
.wedding-header {
  background: #33CC33 url("<%= asset_path('theme/wedding/overlay.png') %>") repeat;
  background-size: 252px 230px;
}
/* Restaurant */
.restaurant .rest-item:hover {
  border-color: #33CC33;
}
/* Icon box 9 */
.icon-box-9 .icon-box-9-item:hover {
  background: #33CC33;
}
/********/
/* Foot */
/********/
.foot {
  border-color: #33CC33;
}
